package hamiltonPath;

import hamiltonCycle.HC;
import classesPb.NP;

public class HP {

	public int nbVilles;
	public boolean[][] distances;
	
	public HP(int nbVilles, boolean[][] distances){
		this.nbVilles=nbVilles;
		this.distances=distances;
	}
	
	public NP red() {
		HC reducPoly = new HC(nbVilles,distances);
		return reducPoly;
	}
}
